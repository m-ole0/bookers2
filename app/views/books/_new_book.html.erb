    <h2>New Book</h2>
      <%= form_with model: book, url: books_path do |f| %>

        <div>Title</div>
      <%= f.text_field :title %>

        <div>Opinion</div>
      <%= f.text_area :body %>

        <div>
      <%= f.submit 'Create Book', class: "btn btn-success" %>
        </div>

      <% end %>